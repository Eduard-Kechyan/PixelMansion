.gameplay_button {
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    transition-duration: 0.1s;
    width: 18px;
    height: 26px;
    background-color: rgba(0, 0, 0, 0);
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
}

.gameplay_button:active {
    scale: 0.8 0.8;
}

.info_box_button {
    font-size: 8px;
    color: rgb(255, 255, 255);
    right: 2px;
    bottom: 2px;
    position: absolute;
    display: none;
}

.info_box_button_value {
    padding-left: 10px;
}

.value_button {
    font-size: 6px;
    -unity-text-align: middle-center;
    color: rgb(58, 58, 58);
    padding-right: 3px;
    padding-left: 20px;
    transition-duration: 0.1s;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    background-color: rgba(0, 0, 0, 0);
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-top: 0;
    padding-bottom: 0;
}

.value_button:active {
    scale: 0.8 0.8;
}

.value_button:disabled {
    opacity: 1;
}

.value_bob {
    scale: 1.2 1.2;
}

.button {
    transition-duration: 0.1s;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    background-color: rgba(0, 0, 0, 0);
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
}

.button_active:active {
    scale: 0.9 0.9;
}

.button_active_extra:active {
    scale: 0.8 0.8;
}

.disabled:disabled {
    opacity: 1;
}

.speech_bubble {
    transition-property: scale, opacity;
    transition-duration: 0.2s, 0.2s;
    transition-timing-function: ease-in-out-elastic, ease-in-out-elastic;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
}

.speech_bubble:active {
    scale: 0.8 0.8;
}

.age_slider .unity-slider-int__label {
    margin-left: 0;
    margin-right: 10px;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    width: 20px;
    height: 26px;
    -unity-font-definition: url('project://database/Assets/Fonts/Roboto%20SDF.asset?fileID=11400000&guid=1c5dedeb0ccefb74ebe6e5ff0a6f7e56&type=2#Roboto SDF');
    color: rgb(51, 51, 51);
    font-size: 14px;
    -unity-text-align: middle-center;
    min-width: 20px;
}

.age_slider .unity-base-slider__tracker {
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    background-color: rgb(197, 188, 163);
    height: 6px;
    max-height: 6px;
    min-height: 6px;
}

.age_slider .unity-base-slider__dragger {
    width: 20px;
    height: 20px;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    margin-top: -10px;
    background-image: url('project://database/Assets/Sprites/Scenes/Loading/Menu/SliderDragger.png?fileID=2800000&guid=aecefaf87cdaa094fb9f95ad7c7f400c&type=3#SliderDragger');
    background-color: rgba(0, 0, 0, 0);
    -unity-background-image-tint-color: rgb(85, 203, 152);
    transition-duration: 0.3s;
}

.age_slider .unity-base-slider__dragger:active {
    -unity-background-image-tint-color: rgb(59, 159, 116);
}

.scroller .unity-scroller {
    width: 4px;
    overflow: hidden;
    margin-right: 4px;
    position: absolute;
    top: 0;
    right: -12px;
    bottom: 0;
}

.scroller .unity-scroller__low-button {
    display: none;
    opacity: 0;
}

.scroller .unity-scroller__high-button {
    display: none;
    opacity: 0;
}

.scroller .unity-scroller__slider {
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    width: 4px;
    overflow: hidden;
}

.scroller .unity-base-slider__tracker {
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    background-color: rgb(227, 213, 179);
    width: 4px;
}

.scroller .unity-base-slider__dragger {
    width: 4px;
    left: 0;
    background-color: rgb(202, 186, 148);
}

.scroller .unity-base-slider__dragger-border {
    background-color: rgba(166, 151, 117, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
}

.age_input .unity-base-field__input {
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
