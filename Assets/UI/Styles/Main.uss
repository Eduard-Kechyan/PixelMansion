.button_standard {
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    transition-duration: 0.1s;
    width: 28px;
    height: 28px;
    background-color: rgba(0, 0, 0, 0);
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    overflow: visible;
}

.button_standard .border {
    position: absolute;
    overflow: visible;
    width: 32px;
    height: 32px;
    left: -2px;
    top: -2px;
    background-image: url('project://database/Assets/Sprites/Buttons/Main/ButtonBorder.png?fileID=2800000&guid=c062e3307eec0304f9afcd865b49cf91&type=3#ButtonBorder');
}

.button_standard:active {
    scale: 0.8 0.8;
}

.button_standard_small {
    width: 20px;
    height: 20px;
    margin-left: auto;
    margin-right: 4px;
    margin-bottom: 6px;
}

.button_standard_small .border {
    width: 24px;
    height: 24px;
    background-image: url('project://database/Assets/Sprites/Buttons/Main/ButtonBorderSmall.png?fileID=2800000&guid=ecb8d5f89de5227409fc70789cfbc449&type=3#ButtonBorderSmall');
    left: -2px;
    top: -2px;
}

.button {
    transition-duration: 0.1s;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    background-color: rgba(0, 0, 0, 0);
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
}

.button_active:active {
    scale: 0.9 0.9;
}

.button_active_extra:active {
    scale: 0.8 0.8;
}

.button_slash {
    left: -100%;
    position: absolute;
    opacity: 0.5;
}

.button_slash_slashing {
    left: 100%;
    transition-duration: 0.5s;
}

.disabled:disabled {
    opacity: 1;
}

.scroller_top_line {
    flex-grow: 1;
    background-color: rgb(219, 205, 168);
    width: 68px;
    height: 2px;
    margin-left: auto;
    margin-right: auto;
    bottom: -1px;
}

.scroller_bottom_line {
    bottom: auto;
    top: -1px;
}

.scroll_view {
    width: 70px;
    height: 44px;
    background-color: rgb(219, 205, 168);
    margin-left: auto;
    margin-right: auto;
    padding-top: 0;
}

.scroller .unity-scroller {
    width: 4px;
    overflow: hidden;
    margin-right: 4px;
    position: absolute;
    top: 0;
    right: -12px;
    bottom: 0;
}

.scroller .unity-scroller__low-button {
    display: none;
    opacity: 0;
}

.scroller .unity-scroller__high-button {
    display: none;
    opacity: 0;
}

.scroller .unity-scroller__slider {
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    width: 4px;
    overflow: hidden;
}

.scroller .unity-base-slider__tracker {
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    background-color: rgb(227, 213, 179);
    width: 4px;
}

.scroller .unity-base-slider__dragger {
    width: 4px;
    left: 0;
    background-color: rgb(202, 186, 148);
}

.scroller .unity-base-slider__dragger-border {
    background-color: rgba(166, 151, 117, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
}

.note_dot {
    width: 8px;
    height: 8px;
    background-image: url('project://database/Assets/Sprites/Buttons/NoteDot.png?fileID=2800000&guid=b85a4c5d66f980a4fbf423fe6413e48b&type=3#NoteDot');
    position: absolute;
    left: -2px;
    top: -2px;
    transition-duration: 0.2s;
    visibility: hidden;
    opacity: 0;
}

.note_dot_small {
    width: 6px;
    height: 6px;
    background-image: url('project://database/Assets/Sprites/Buttons/NoteDotSmall.png?fileID=2800000&guid=87f87a41fbc83544584f9c639283f5d9&type=3#NoteDotSmall');
    position: absolute;
    left: -1px;
    top: -1px;
    transition-duration: 0.1s;
    visibility: hidden;
    opacity: 0;
}

.note_dot .value {
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    -unity-text-align: middle-center;
    color: rgb(255, 255, 255);
    font-size: 6px;
    width: 8px;
    height: 8px;
}

.note_dot_bloop {
    scale: 1.5 1.5;
}

.splash_container {
    position: absolute;
    left: 0;
    top: 50%;
    right: 0;
    background-image: url('project://database/Assets/Sprites/Scenes/Gameplay/Background.png?fileID=2800000&guid=67e0e2f1b7ca3854b95261c156f6d02b&type=3#Background');
    height: 500px;
    width: 180px;
    translate: 0 -50%;
}

.splash_logo {
    position: absolute;
    left: 50%;
    top: 50%;
    width: 64px;
    height: 64px;
    translate: -50% -50%;
    background-image: url('project://database/Assets/Sprites/Splash/Logo.png?fileID=2800000&guid=a321d0e6c5ec8a741aaa95aac3f573f4&type=3#Logo');
    transition-duration: 0.5s;
    transition-timing-function: ease-in-out;
}

.debug_button {
    position: absolute;
    width: 20px;
    height: 20px;
    right: 22px;
    bottom: 20px;
}
