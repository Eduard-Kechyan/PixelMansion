    font-size: 8px;
}

.locale_hy .menu_title_value {
    font-size: 10px;
}

.locale_jp {
    -unity-font-definition: url('project://database/Assets/Fonts/NotoJapanese%20SDF.asset?fileID=11400000&guid=4886decfe93756a4f893a0442c159960&type=2#NotoJapanese SDF');
    font-size: 8px;
