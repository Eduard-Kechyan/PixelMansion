.locale_jp .menu_title_value {
    font-size: 10px;
}

.locale_kr {
    -unity-font-definition: url('project://database/Assets/Fonts/NotoKorean%20SDF.asset?fileID=11400000&guid=fe1d837d128d95540b9f921dc6562a90&type=2#NotoKorean SDF');
    font-size: 8px;
